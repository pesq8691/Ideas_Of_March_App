<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="z.y.x.ideasofmarchapp"
   >

    <uses-permission android:name="android.permission.INTERNET" />

    <!--Spinner-->


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayWorkActivity"
            android:label="@string/title_activity_display_button"
            android:parentActivityName=".LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="z.y.x.ideasofmarchapp.LoginActivity" />
        </activity>
        <activity
            android:name=".Assignments"
            android:label="@string/title_activity_assignments" >
        </activity>
        <activity
            android:name=".Buttom"
            android:label="@string/title_activity_buttom" >
        </activity>

        <!--Spinner-->



    </application>

</manifest>
